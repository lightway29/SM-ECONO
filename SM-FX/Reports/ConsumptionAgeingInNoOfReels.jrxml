	<parameter name="to_time_stamp" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT m1.item_no,IFNULL(m1.1month,0) as 1month
,IFNULL(m2.2month,0) as 2month
,IFNULL(m3.3month,0) as 3month
,IFNULL(m4.4month,0) as 4month
,IFNULL(m5.5month,0) as 5month
,IFNULL(m6.6month,0) as 6month
,IFNULL(m7.7month,0) as 7month
,(1month+2month+3month+4month+5month+6month+7month) as total
FROM (SELECT item_no, count(issue_weight-return_weight) as 1month
FROM sm_db.reel_log rl
right join sm_db.reel r
on r.reel_code=rl.reel_code && rl.return_time_stamp between DATE_SUB($P{to_time_stamp}, INTERVAL 1 MONTH) and $P{to_time_stamp}
group by r.item_no
order by item_no) as m1,
(SELECT item_no, count(issue_weight-return_weight) as 2month
FROM sm_db.reel_log rl
right join sm_db.reel r
on r.reel_code=rl.reel_code && rl.return_time_stamp between DATE_SUB($P{to_time_stamp}, INTERVAL 3 MONTH) and DATE_SUB(DATE_SUB($P{to_time_stamp}, INTERVAL 1 MONTH), INTERVAL 1 DAY)
group by item_no
order by item_no) as m2,
(SELECT item_no, count(issue_weight-return_weight) as 3month
FROM sm_db.reel_log rl
right join sm_db.reel r
on r.reel_code=rl.reel_code && rl.return_time_stamp between DATE_SUB($P{to_time_stamp}, INTERVAL 6 MONTH) and DATE_SUB(DATE_SUB($P{to_time_stamp}, INTERVAL 3 MONTH), INTERVAL 1 DAY)
group by r.item_no
order by item_no) as m3,
(SELECT item_no, count(issue_weight-return_weight) as 4month
FROM sm_db.reel_log rl
right join sm_db.reel r
on r.reel_code=rl.reel_code && rl.return_time_stamp between DATE_SUB($P{to_time_stamp}, INTERVAL 9 MONTH) and DATE_SUB(DATE_SUB($P{to_time_stamp}, INTERVAL 6 MONTH), INTERVAL 1 DAY)
group by r.item_no
order by item_no) as m4,
(SELECT item_no, count(issue_weight-return_weight) as 5month
FROM sm_db.reel_log rl
right join sm_db.reel r
on r.reel_code=rl.reel_code && rl.return_time_stamp between DATE_SUB($P{to_time_stamp}, INTERVAL 12 MONTH) and DATE_SUB(DATE_SUB($P{to_time_stamp}, INTERVAL 9 MONTH), INTERVAL 1 DAY)
group by r.item_no
order by item_no) as m5,
(SELECT item_no, count(issue_weight-return_weight) as 6month
FROM sm_db.reel_log rl
right join sm_db.reel r
on r.reel_code=rl.reel_code && rl.return_time_stamp between DATE_SUB($P{to_time_stamp}, INTERVAL 24 MONTH) and DATE_SUB(DATE_SUB($P{to_time_stamp}, INTERVAL 12 MONTH), INTERVAL 1 DAY)
group by r.item_no
order by item_no) as m6,
(SELECT item_no, count(issue_weight-return_weight) as 7month
FROM sm_db.reel_log rl
right join sm_db.reel r
on r.reel_code=rl.reel_code && rl.return_time_stamp < DATE_SUB($P{to_time_stamp}, INTERVAL 24 MONTH)
group by r.item_no
order by item_no) as m7
where  m1.item_no=m2.item_no && m2.item_no=m3.item_no && m3.item_no=m4.item_no && m4.item_no=m5.item_no && m5.item_no=m6.item_no && m6.item_no=m7.item_no]]>
	</queryString>
