	<queryString>
		<![CDATA[SELECT r.*,l.time_stamp as issue_time_stamp,
l.issue_weight, l.return_weight, l.return_time_stamp
FROM reel_log l
inner join reel r
on l.reel_code = r.reel_code
where DATE(l.time_stamp) BETWEEN  $P{from_time_stamp} AND $P{to_time_stamp}]]>
	</queryString>
